<h1>Listing photos</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Prey common</th>
    <th>Prey latin</th>
    <th>Bird common</th>
    <th>Bird latin</th>
    <th>Location friendly</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @photos.each do |photo| %>
  <tr>
    <td><%= photo.name %></td>
    <td><%= photo.email %></td>
    <td><%= photo.prey_common %></td>
    <td><%= photo.prey_latin %></td>
    <td><%= photo.bird_common %></td>
    <td><%= photo.bird_latin %></td>
    <td><%= photo.location_friendly %></td>
    <td><%= link_to 'Show', photo %></td>
    <td><%= link_to 'Edit', edit_photo_path(photo) %></td>
    <td><%= link_to 'Destroy', photo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Photo', new_photo_path %>
